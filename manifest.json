{
  "manifest_version": 3,
  "name": "Medium parser",
  "version": "1.3.1",
  "description": "Unlocks the whole medium article on the go.",
  "icons": {
    "16": "img/icon16.jpg",
    "32": "img/icon32.jpg",
    "48": "img/icon48.jpg",
    "64": "img/icon64.jpg",
    "128": "img/icon128.jpg",
    "96": "img/icon96.jpg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "medium-parser@example.com"
    }
  }
}
